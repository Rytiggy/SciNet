{  
   "manifest_version":2,
   "name":"SciNet",
   "version": "0.0.1",
  "background": {
    "persistent": false,
    "scripts": ["/assets/js/background.js"]
  },
   "content_scripts":[  
      {  
         "matches":[  
            "<all_urls>"
         ],
         "js":[  
            "/assets/js/jquery-3.1.1.js",
            "/assets/js/content.js"
         ]
      }
   ],
   "permissions": [
      "tabs"
  ],
   "browser_action":{  
      "default_icon":"assets/img/icon.png",
      "default_popup":"popup.html"
   }

}